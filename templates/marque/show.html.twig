{% extends 'base.html.twig' %}

{% block title %}Marque{% endblock %}

{% block body %}
    <h1>{% trans %}Marque{% endtrans %}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>{% trans %}IdMarque{% endtrans %}</th>
                <td>{{ marque.idMarque }}</td>
            </tr>
            <tr>
                <th>{% trans %}NomMarque{% endtrans %}</th>
