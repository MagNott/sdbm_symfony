    <a href="{{ path('app_marque_index') }}">{% trans %}back to list{% endtrans %}</a>

    <a href="{{ path('app_marque_edit', {'idMarque': marque.idMarque}) }}">{% trans %}edit{% endtrans %}</a>

    {{ include('marque/_delete_form.html.twig') }}
{% endblock %}
